{
  "name": "express-vite-ssr",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build --outDir dist/client && vite build --ssr server/ssr-render.ts --outDir dist/server",
    "ci:eslint": "eslint -f json src -o ./.ci/eslint.json",
    "dev": "tsx server/index.tsx",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start": "NODE_ENV=production tsx server/index.tsx"
  },
  "dependencies": {
    "express": "^4.21.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^5.0.1",
    "eslint": "^9.34.0",
    "eslint-config-ali": "^16.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
